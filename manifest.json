{
    "manifest_version": 3,
    "name": "Danish leads",
    "description": "Show apollo user name when clicked.",
    "version": "1.0.0",
    "permissions": ["activeTab"],
    "host_permissions": [
      "https://app.apollo.io/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
      }
    },
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
      },
 
    "content_scripts": [
      {
        "matches": ["https://app.apollo.io/*"],
        "js": ["content.js"]
      }
    ]
  }
  